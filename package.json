		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.1.1",
		"vitest": "^1.2.2"
	},
	"type": "module",
	"dependencies": {
		"@capacitor/android": "^5.7.0",
		"@capacitor/core": "^5.7.0",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"bits-ui": "^0.17.0",
		"clsx": "^2.1.0",
		"embla-carousel-svelte": "8.0.0-rc22",
		"mode-watcher": "^0.2.0",
		"pocketbase": "^0.21.1",
		"svelte-icons": "^2.1.0",
		"svelte-motion": "^0.12.1",
		"svelte-radix": "^1.0.1",
		"tailwind-merge": "^2.2.1",
